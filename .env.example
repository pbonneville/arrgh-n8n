# Google Cloud Configuration
PROJECT_ID=your-gcp-project-id
REGION=us-central1
REPO_NAME=n8n-repo
SERVICE_NAME=n8n-app

# n8n Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your-secure-password
N8N_API_KEY=your-n8n-api-key

# Database Configuration (Cloud SQL)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=35.225.58.181
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=postgres
DB_POSTGRESDB_PASSWORD=your-cloud-sql-password
DB_POSTGRESDB_SSL_ENABLED=false

# n8n Settings
N8N_HOST=0.0.0.0
N8N_PORT=5678
N8N_PROTOCOL=https
WEBHOOK_URL=https://your-render-url.onrender.com/
N8N_ENCRYPTION_KEY=your-encryption-key

# Execution Settings
EXECUTIONS_MODE=regular
N8N_METRICS=true

# Email Configuration (AWS SES)
N8N_EMAIL_MODE=smtp
N8N_SMTP_HOST=email-smtp.us-east-1.amazonaws.com
N8N_SMTP_PORT=587
N8N_SMTP_SSL=false
N8N_SMTP_STARTTLS=true
N8N_DEFAULT_EMAIL=paul@paulbonneville.com
N8N_DEFAULT_EMAIL_FROM_NAME=n8n
N8N_SMTP_SENDER=paul@paulbonneville.com
N8N_SMTP_USER=your-aws-ses-username
N8N_SMTP_PASS=your-aws-ses-password

# API Keys for integrations
ANTHROPIC_API_KEY=your-anthropic-api-key
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key